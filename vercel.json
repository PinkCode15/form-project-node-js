{
    "version": 2,
    "builds": [{"src":"./server.js", "use": "@vercel/node"}],
    "routes": [
        {"src": "/index.html", "dest": "/index.html"},
        {"src": "/index.css", "dest": "/index.css"},
        {"src": "/script.js", "dest": "/script.js"},
        {"src": "/(.*)", "dest": "/index.html"}
    ]
}